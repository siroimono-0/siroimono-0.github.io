```cpp
#include <bits/stdc++.h>
#include <cstdint>
#include <execinfo.h>
#include <pthread.h>

using namespace std;

void print_bk()
{
  void *p_stack[64];
  int ssize = backtrace(p_stack, 64);
  char **pp_str = backtrace_symbols(p_stack, ssize);

  cout << "==========tmptmp==========" << endl;
  for (int i = 0; i < ssize; i++)
  {
    cout << pp_str[i] << endl;
  }
  cout << "==========tmptmp==========" << endl;
}

void rere(int ss)
{
  if (ss == 0)
  {
    print_bk();
    return;
  }

  rere(ss - 1);
  return;
}
int main()
{
  // char **buf = (char **)malloc(buf, 1024);
  // int ret = backtrace(, 10);
  rere(5);

  return 0;
}
```
